{
  "name": "beds24-hub-api",
  "version": "0.0.1",
  "description": "api hub to allow connection between different services and beds24",
  "main": "app.js",
  "repository": "https://github.com/angelo-basso/beds24-api.git",
  "author": "Angelo BASSO",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "dotenv -- tsc-watch --sourceMap --onsuccess \"node --enable-source-maps dist/index.js\"",
