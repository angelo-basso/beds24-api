    "migrate": "dotenv -- node migrate up",
    "migrate:rollback": "dotenv -- node migrate down",
    "migration:create": "dotenv -- node migrate create"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.4.7",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "ts-node": "^10.9.2",
    "umzug": "^3.8.2"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.15.21",
    "@types/validator": "^13.15.4",
    "dotenv-cli": "^8.0.0",
    "globals": "^16.1.0",
    "tsc-watch": "^6.3.1",
    "typescript": "^5.8.3"
  }
